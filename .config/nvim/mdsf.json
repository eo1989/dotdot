{
  "$schema": "https://raw.githubusercontent.com/hougesen/mdsf/main/schemas/v0.10.5/mdsf.schema.json",
  "languages": {
    "c": "clang-format",
    "cpp": "clang-format",
    "json": ["biome:lint", "biome:check", "biome:format", "prettier"],
    "jsonc": ["biome:lint", "biome:check", "biome:format", "prettier"],
    "julia": ["juliaformatter.jl", "runic"],
    "python": [ "ruff:check", "ruff:format" ],
    "shell": [
      [
        "shfmt",
        "beautysh"
      ]
    ],
    "sql": [
      [
        "sqlfluff:fix",
        "sqlfluff:lint",
        "sqlfluff:format",
        "sql-formatter",
        "sqlint",
        "sqlfmt",
        "sqruff",
        "sleek"
      ]
    ]
  }
}
